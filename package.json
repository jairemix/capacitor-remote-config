{
  "name": "@jairemix/capacitor-remote-config",
  "version": "0.0.1",
  "private": true,
  "description": "Capacitor wrapper plugin for Firebase Remote Config SDK",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "yarn build",
    "release": "yarn publish --access public"
  },
  "author": "Jeremy Li",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^3.2.4",
    "@capacitor/ios": "^1.0.0",
    "@capacitor/android": "^1.0.0"
  },
  "files": [
    "dist/",
    "ios/Plugin",
    "ios/Plugin.xcodeproj",
    "ios/Plugin.xcworkspace",
    "ios/PluginTests",
    "ios/Podfile",
    "ios/Podfile.lock",
    "android/",
    "JairemixCapacitorRemoteConfig.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jairemix/capacitor-remote-config"
  },
  "bugs": {
    "url": "https://github.com/jairemix/capacitor-remote-config/issues"
  }
}
